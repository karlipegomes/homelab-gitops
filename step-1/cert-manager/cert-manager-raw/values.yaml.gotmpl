resources:
  - apiVersion: external-secrets.io/v1
    kind: ClusterExternalSecret
    metadata:
      name: gcp-dns-cred
    spec:
      externalSecretSpec:
        secretStoreRef:
          kind: SecretStore
          name: gcp-dns-cred
        target:
          name: cert-manager-cred
        data:
        - secretKey: credentials.json
          remoteRef:
            key: homelab-dns-credentials
