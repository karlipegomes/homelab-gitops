resources:
  - apiVersion: external-secrets.io/v1alpha1
    kind: ExternalSecret
    metadata:
      name: cert-manager-cred
    spec:
      secretStoreRef:
        kind: ClusterSecretStore
        name: staffops-store
      target:
        name: cert-manager-cred
      data:
      - secretKey: credentials.json
        remoteRef:
          key: homelab-dns-credentials
