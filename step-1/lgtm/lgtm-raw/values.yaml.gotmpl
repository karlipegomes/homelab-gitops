resources:
  - apiVersion: external-secrets.io/v1
    kind: ExternalSecret
    metadata:
      name: grafana-admin
    spec:
      secretStoreRef:
        kind: ClusterSecretStore
        name: staffops-store
      target:
        name: grafana-admin
      data:
        - secretKey: admin-user
          remoteRef:
            key: homelab-grafana-user
        - secretKey: admin-password
          remoteRef:
            key: homelab-grafana-pass
            